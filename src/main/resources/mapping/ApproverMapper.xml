<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="bingosoft.hrhelper.mapper.ApproverMapper" >
  <resultMap id="BaseResultMap" type="bingosoft.hrhelper.model.Approver" >
    <result column="username" property="username" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="bingosoft.hrhelper.model.Approver" >
    insert into approver (username)
    values (#{username,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="bingosoft.hrhelper.model.Approver" >
    insert into approver
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="username != null" >
        username,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="username != null" >
        #{username,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>